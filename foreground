div{
  width: 100px;
  height: 100px;
  border-radius: 100px;
  border:20px solid blue ;
  border-top: 20px solid black;
  margin: 300px;
  /* position: absolute; */
  transform: rotate(0deg);
  animation: spin 1s steps(4) 9s infinite normal;
  }
  
  @keyframes spin {
     0%{
      transform: rotate(0deg);
     }
     50%{
      transform: rotate(180deg);
     }
     100%{
      transform: rotate(300deg);
     }
  
  }
  